<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>ServletHello</servlet-name>
        <servlet-class>ServletHello</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Servlet_ServletConfig</servlet-name>
        <servlet-class>Servlet_ServletConfig</servlet-class>
        <init-param>
            <param-name>repeticiones</param-name>
            <param-value>5</param-value>
        </init-param>
        <init-param>
            <param-name>mensaje</param-name>
            <param-value>Hola esta es una prueba...</param-value>
        </init-param>
    </servlet>
    <servlet>
        <servlet-name>NewServlet</servlet-name>
        <servlet-class>Mipaquete.NewServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Servlet_LeyendoNavegador</servlet-name>
        <servlet-class>Servlet_LeyendoNavegador</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Servlet_LeyendoCabezeras</servlet-name>
        <servlet-class>Servlet_LeyendoCabezeras</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletDespliegaNombreApellido</servlet-name>
        <servlet-class>ServletDespliegaNombreApellido</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Servlet_LeyendoDatosEnFormaGenerica</servlet-name>
        <servlet-class>Servlet_LeyendoDatosEnFormaGenerica</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>tamanoBuffer</servlet-name>
        <servlet-class>tamanoBuffer</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Servlet_TamanoBuffer</servlet-name>
        <servlet-class>Servlet_TamanoBuffer</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Servlet_CodigosEstado</servlet-name>
        <servlet-class>Servlet_CodigosEstado</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Sevlet_CodigoDeError</servlet-name>
        <servlet-class>Sevlet_CodigoDeError</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Servlet_CreateCookie</servlet-name>
        <servlet-class>Servlet_CreateCookie</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Servlet_ShowCookieValues</servlet-name>
        <servlet-class>Servlet_ShowCookieValues</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Servlet_DaCookies</servlet-name>
        <servlet-class>Servlet_DaCookies</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletHello</servlet-name>
        <url-pattern>/MiServletHello/DatosExtra/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Servlet_ServletConfig</servlet-name>
        <url-pattern>/Servlet_ServletConfig</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NewServlet</servlet-name>
        <url-pattern>/NewServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Servlet_LeyendoNavegador</servlet-name>
        <url-pattern>/Servlet_LeyendoNavegador/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Servlet_LeyendoCabezeras</servlet-name>
        <url-pattern>/Servlet_LeyendoCabezeras</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletDespliegaNombreApellido</servlet-name>
        <url-pattern>/ServletDespliegaNombreApellido</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Servlet_LeyendoDatosEnFormaGenerica</servlet-name>
        <url-pattern>/Servlet_LeyendoDatosEnFormaGenerica</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>tamanoBuffer</servlet-name>
        <url-pattern>/tamanoBuffer</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Servlet_TamanoBuffer</servlet-name>
        <url-pattern>/Servlet_TamanoBuffer</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Servlet_CodigosEstado</servlet-name>
        <url-pattern>/Servlet_CodigosEstado</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Sevlet_CodigoDeError</servlet-name>
        <url-pattern>/Sevlet_CodigoDeError</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Servlet_CreateCookie</servlet-name>
        <url-pattern>/Servlet_CreateCookie</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Servlet_ShowCookieValues</servlet-name>
        <url-pattern>/Servlet_ShowCookieValues</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Servlet_DaCookies</servlet-name>
        <url-pattern>/Servlet_DaCookies</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
