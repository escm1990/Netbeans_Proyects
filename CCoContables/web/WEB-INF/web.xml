        <servlet-class>controladores.AlmacenarSucursal</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ListadoCuentas</servlet-name>
        <servlet-class>controladores.ListadoCuentas</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ManejoDatos</servlet-name>
        <servlet-class>controladores.ManejoDatos</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>cerrarSesion</servlet-name>
        <servlet-class>controladores.cerrarSesion</servlet-class>
    </servlet>
    <servlet>
