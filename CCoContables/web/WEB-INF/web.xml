        <servlet-class>ControladoresRatios.ServletEndeudamiento</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletEndeudamientoCorto</servlet-name>
        <servlet-class>ControladoresRatios.ServletEndeudamientoCorto</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletEndeudamientoLargo</servlet-name>
        <servlet-class>ControladoresRatios.ServletEndeudamientoLargo</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AlmacenarCuentas</servlet-name>
        <url-pattern>/AlmacenarCuentas</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AlmacenarSucursal</servlet-name>
        <url-pattern>/AlmacenarSucursal</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListadoCuentas</servlet-name>
        <url-pattern>/ListadoCuentas</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ManejoDatos</servlet-name>
        <url-pattern>/ManejoDatos</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>cerrarSesion</servlet-name>
        <url-pattern>/cerrarSesion</url-pattern>
    </servlet-mapping>
